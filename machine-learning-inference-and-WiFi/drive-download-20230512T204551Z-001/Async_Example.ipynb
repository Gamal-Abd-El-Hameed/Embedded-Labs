   "source": [
    "import asyncio\n",
    "async def countAsync(task_no):\n",
    "    for i in range(0,4):\n",
    "        print(f\"Task {task_no}:\" + str(i))\n",
    "    return task_no * 6\n",
    "tasks = []\n",
    "for i in range(0,4):\n",
    "    task = asyncio.create_task(countAsync(i+1))\n",
    "    tasks.append(task)\n",
    "await asyncio.gather(*tasks)"
   ]
