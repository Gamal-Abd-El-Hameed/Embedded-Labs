    "import asyncio\n",
    "async def countAsync():\n",
    "    for i in range(0,4):\n",
    "        print(i)\n",
    "    return i+1\n",
    "tasks = []\n",
    "for i in range(0,4):\n",
    "    task = asyncio.create_task(countAsync())\n",
    "    tasks.append(task)\n",
    "await asyncio.gather(*tasks)"
   ]
